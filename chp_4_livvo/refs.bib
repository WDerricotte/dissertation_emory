@article{otero_nitrogen_2006,
	title = {Nitrogen 1s {Near}-{Edge} {X}-ray {Absorption} {Fine} {Structure} {Spectroscopy} of {Amino} {Acids}: {Resolving} {Zwitterionic} {Effects}},
	volume = {110},
