	volume = {76},
	issn = {1097-461X},
	url = {http://onlinelibrary.wiley.com.proxy.library.emory.edu/doi/10.1002/(SICI)1097-461X(2000)76:2<169::AID-QUA7>3.0.CO;2-G/abstract},
