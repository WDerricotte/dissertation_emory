@article{sax_localization_2012,
	title = {Localization of molecular orbitals on fragments},
	volume = {33},
	issn = {1096-987X},
	url = {http://onlinelibrary.wiley.com.proxy.library.emory.edu/doi/10.1002/jcc.22980/abstract},
	doi = {10.1002/jcc.22980},
	abstract = {A non-iterative algorithm for the localization of molecular orbitals (MOs) from complete active space self consistent field (CASSCF) and for single-determinantal wave functions on predefined moieties is given. The localized fragment orbitals can be used to analyze chemical reactions between fragments and also the binding of fragments in the product molecule with a fragments-in-molecules approach by using a valence bond expansion of the CASSCF wave function. The algorithm is an example of the orthogonal Procrustes problem, which is a matrix optimization problem using the singular value decomposition. It is based on the similarity of the set of MOs for the moieties to the localized MOs of the molecule; the similarity is expressed by overlap matrices between the original fragment MOs and the localized MOs. For CASSCF wave functions, localization is done independently in the space of occupied orbitals and active orbitals, whereas, the space of virtual orbitals is mostly uninteresting. Localization of Hartree–Fock or Kohn–Sham density functional theory orbitals is not straightforward; rather, it needs careful consideration, because in this case some virtual orbitals are needed but the space of virtual orbitals depends on the basis sets used and causes considerable problems due to the diffuse character of most virtual orbitals. © 2012 Wiley Periodicals, Inc.},
	language = {en},
	number = {17},
	urldate = {2017-01-24},
	journal = {J. Comput. Chem.},
	author = {Sax, Alexander F.},
	month = jun,
	year = {2012},
	keywords = {fragment, localized, non-iterative algorithm, orbitals, orthogonal Procrustes problem},
	pages = {1495--1510},
	file = {Full Text PDF:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\9W6SPHMM\\Sax - 2012 - Localization of molecular orbitals on fragments.pdf:application/pdf;Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\ERI4UM4T\\abstract.html:text/html}
