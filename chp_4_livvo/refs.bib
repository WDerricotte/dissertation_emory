	journal = {Thin Solid Films},
	author = {Roy, S. S. and McCann, R. and Papakonstantinou, P. and Maguire, P. and McLaughlin, J. A.},
	month = jun,
	year = {2005},
	keywords = {Carbon nitride, Cathodic arc, Thin film, X-ray spectroscopy},
	pages = {145--150},
