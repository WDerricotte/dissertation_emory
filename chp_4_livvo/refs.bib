@article{laikov_intrinsic_2011-1,
	title = {Intrinsic minimal atomic basis representation of molecular electronic wavefunctions},
	volume = {111},
	issn = {1097-461X},
	url = {http://onlinelibrary.wiley.com.proxy.library.emory.edu/doi/10.1002/qua.22767/abstract},
	doi = {10.1002/qua.22767},
	abstract = {The problem of finding an effective minimal atomic basis that spans the exact occupied wavefunctions of a mean-field theory at a given molecular geometry, which has a number of special properties, is studied and a new general procedure is developed that (1) solves for a raw minimal set of strongly atom-centered functions—products of spherical harmonics and molecule-optimized radial parts—that approximately span the occupied molecular wavefunctions and minimize the sum of their energies, (2) uses projection operators to get a new set of deformed atom-centered functions that exactly span the occupied space and fall into core and valence subsets, (3) applies a new zero-bond-dipole orthogonalization scheme to the core-orthogonalized valence subset so that for each two-center product of these functions the projection of its dipole moment along the line going through the two centers is zero. The resulting effective minimal atomic basis is intrinsic to the molecular problem and does not need a free-atoms input. Some interesting features of the zero-bond-dipole orthogonalization are showing up in the atomic population analysis of a diverse set of molecules. The new procedure may be useful for the interpretation of electronic structure, for the construction of model Hamiltonians in terms of transferable molecular integrals, and for the definition of active valence space in the treatment of electron correlation. © 2010 Wiley Periodicals, Inc. Int J Quantum Chem, 2011},
	language = {en},
	number = {12},
	urldate = {2017-01-24},
	journal = {Int. J. Quantum Chem.},
	author = {Laikov, Dimitri N.},
	month = oct,
	year = {2011},
	keywords = {localized orthogonalization, minimal atomic basis, model Hamiltonian, parametrized electronic structure, transferable integrals},
	pages = {2851--2867},
	file = {Full Text PDF:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\9E5E4VQF\\Laikov - 2011 - Intrinsic minimal atomic basis representation of m.pdf:application/pdf;Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\I8NIG78C\\abstract.html:text/html}
