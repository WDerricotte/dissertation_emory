	month = sep,
	year = {2016},
	pages = {6944--6955},
	file = {ACS Full Text PDF w/ Links:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\3TXQ5FID\\Valchanov et al. - 2016 - Understanding the Fluorescence of TADF Light-Emitt.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\3GK53928\\acs.jpca.html:text/html}
