@article{king_corresponding_1967,
	title = {Corresponding {Orbitals} and the {Nonorthogonality} {Problem} in {Molecular} {Quantum} {Mechanics}},
	volume = {47},
	issn = {0021-9606},
	url = {http://aip.scitation.org.proxy.library.emory.edu/doi/abs/10.1063/1.1712221},
	doi = {10.1063/1.1712221},
	abstract = {Given any two sets of spin orbitals ai and bj, there exist equivalent sets {\^a}i and b̂j such that their overlap matrix is diagonal, i.e., 〈{\^a}i {\textbar} b̂j〉=d̂iiδij. This is the basis of the corresponding orbital transformation of Amos and Hall. Their transformation is shown to have widespread application to quantum chemistry. It leads to a simple generalization of the Slater—Condon rules for the expectation value of an operator between two determinantal wavefunctions when the spin orbitals of one function have no simple orthogonality relationship to those of the other function. In the case of single‐determinantal wavefunctions, use of the corresponding orbital transformation and the integral Hellmann—Feynman formula leads to a very simple expression for the energy difference associated with two similar configurations of a molecular system. Extensions to limited configuration interaction expansions are discussed. Given single‐determinantal wavefunctions for two related molecular systems, it is shown that the corresponding orbitals are those which are most nearly molecularly invariant in the sense of maximum overlap. A comparison of the Pitzer—Lipscomb wavefunctions for the staggered and eclipsed forms of ethane reveals that six of the nine corresponding orbitals have an overlap of no less than 0.999998 in the two configurations. Use of the corresponding orbital transformation overcomes various computational difficulties encountered with L{\"o}wdin's cofactor method for treating the nonorthogonality problem.},
	number = {6},
	urldate = {2017-01-24},
	journal = {The Journal of Chemical Physics},
	author = {King, H. F. and Stanton, R. E. and Kim, H. and Wyatt, R. E. and Parr, R. G.},
	month = sep,
	year = {1967},
	pages = {1936--1941},
	file = {Full Text PDF:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\D8W833NC\\1967 - Corresponding Orbitals and the Nonorthogonality Pr.pdf:application/pdf;Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\NFKPQITX\\1.html:text/html}
