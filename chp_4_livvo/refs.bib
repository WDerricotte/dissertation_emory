@article{behyan_chemical_2014,
	title = {Chemical Sensitivity of Sulfur 1s {NEXAFS} Spectroscopy {I}: {Speciation} of Sulfoxides and Sulfones},
	volume = {592},
	issn = {0009-2614},
	shorttitle = {Chemical sensitivity of sulfur 1s {NEXAFS} spectroscopy {I}},
