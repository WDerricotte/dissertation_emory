	keywords = {External quantum efficiency, Material design, Optical properties, Organic light emitting diodes, Organic solar cells, Singlet–triplet gaps},
	pages = {985--991},
	file = {ScienceDirect Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\UMMDZTNH\\S156611991200081X.html:text/html}
