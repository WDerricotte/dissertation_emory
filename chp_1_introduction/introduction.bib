@article{west_comprehensive_2015-2,
	title = {A {Comprehensive} {Analysis} in {Terms} of {Molecule}-{Intrinsic}, {Quasi}-{Atomic} {Orbitals}. {II}. {Strongly} {Correlated} {MCSCF} {Wave} {Functions}},
	volume = {119},
	issn = {1089-5639},
