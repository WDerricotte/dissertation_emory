@article{kim_effects_2015,
	title = {Effects of {Intermolecular} {Interactions} on the {Singlet}–{Triplet} {Energy} {Difference}: {A} {Theoretical} {Study} of the {Formation} of {Excimers} in {Acene} {Molecules}},
	volume = {119},
