@article{rangan_experimental_2005,
	title = {Experimental and theoretical {NEXAFS}/{XPS} study of the room-temperature adsorption of acetonitrile on \${\textbackslash}mathrm\{{Si}\}(001){\textbackslash}text\{{\textbackslash}ensuremath\{-\}\}2{\textbackslash}ifmmode{\textbackslash}times{\textbackslash}else{\textbackslash}texttimes{\textbackslash}fi\{\}1\$},
