	file = {Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\FGWNFSXZ\\00268970500417762.html:text/html}
