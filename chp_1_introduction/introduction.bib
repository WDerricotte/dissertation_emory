	volume = {417},
	issn = {0039-6028},
	shorttitle = {Cycloaddition reactions of unsaturated hydrocarbons on the {Si}(100)-(2×1) surface},
	url = {http://www.sciencedirect.com/science/article/pii/S0039602898005548},
	doi = {10.1016/S0039-6028(98)00554-8},
	abstract = {First-principles electronic structure calculations have been used to study the structure, energetics and vibrational spectra of the chemisorption products of several unsaturated hydrocarbons on the Si(100)-(2×1) surface. The calculations use a hybrid non-local density functional theory and a cluster model of the surface. Ethylene and acetylene react by a [2s+2s] cycloaddition mechanism. Conjugated dienes (1,3-cyclohexadiene, 1,3-butadiene, 2,3-dimethyl-1,3-butadiene) and benzene can also react by a novel [4s+2s] cycloaddition, or Diels–Alder mechanism. For each diene, the Diels–Alder product is energetically favored over the more strained [2s+2s] product. The reaction mechanism for Diels–Alder addition, other competing reactions, and the effects of post-hydrogenation are all discussed. Comparisons to experimental observations are made throughout.},
	number = {2–3},
	journal = {Surf. Sci.},
	author = {Konečný, R. and Doren, D. J.},
	month = nov,
	year = {1998},
	keywords = {Alkenes, Alkynes, Aromatics, density functional calculations, Dienes, Silicon, Surface chemical reaction},
	pages = {169--188},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\Z5JFFKKX\\Konečný and Doren - 1998 - Cycloaddition reactions of unsaturated hydrocarbon.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\ZTIE4CH6\\S0039602898005548.html:text/html}
