@article{tanaka_observation_2005,
	title = {Observation of phosphorescence from tris(8-hydroxyquinoline) aluminum thin films using triplet energy transfer from iridium complexes},
	volume = {71},
	url = {http://link.aps.org/doi/10.1103/PhysRevB.71.205207},
	doi = {10.1103/PhysRevB.71.205207},
	abstract = {Strong phosphorescence from tris(8-hydroxyquinoline) aluminum (Alq3) was observed in amorphous Alq3 thin films doped with phosphorescent molecules, fac tris(2-phenylpyridine) iridium [Ir(ppy)3]. The energy-transfer and light-emission mechanisms were studied in the Ir(ppy)3-doped Alq3 thin films. The triplet energy of Alq3 was determined to be 2.03eV from the highest-energy peak of the phosphorescence spectra, and the phosphorescence lifetime was 5.3ms at 8K. The phosphorescence lifetime and intensity significantly decreased with an increase in temperature, especially at temperatures above 100K. From this study, we have found that the triplet energy transfer from Ir(ppy)3 enables us to characterize the phosphorescence properties of Alq3, and that Ir(ppy)3 plays the important role of a phosphorescent sensitizer for Alq3.},
	number = {20},
