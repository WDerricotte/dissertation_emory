	keywords = {boron, OLEDs, photoluminescence, thermally activated delay fluorescence},
	pages = {3017--3021},
	file = {Full Text PDF:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\NWRP3A9T\\Shiu et al. - 2016 - Pyridyl Pyrrolide Boron Complexes The Facile Gene.pdf:application/pdf;Snapshot:C\:\\Users\\WDerricotte\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\w08m8rxy.default\\zotero\\storage\\6PV3DBMV\\abstract.html:text/html}
